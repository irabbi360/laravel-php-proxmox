{
    "name": "irabbi360/php-proxmox",
    "description": "A PHP package for Proxmox API integration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Irabbi360\\Proxmox\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Irabbi360\\Proxmox\\ProxmoxServiceProvider"
            ],
            "aliases": {
                "Proxmox": "Irabbi360\\Proxmox\\Facades\\ProxmoxVM"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
